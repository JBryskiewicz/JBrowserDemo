<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="900.0" prefWidth="1800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.me.jbrowserdemo.HomepageController">
   <children>
      <WebView fx:id="webView" layoutX="10.0" layoutY="100.0" prefHeight="845.0" prefWidth="1780.0" />
      <Button fx:id="reloadButton" layoutX="135.0" layoutY="59.0" mnemonicParsing="false" onAction="#reloadPage" prefHeight="25.0" prefWidth="61.0" text="Reload" textAlignment="CENTER" />
      <TextField fx:id="urlField" layoutX="196.0" layoutY="59.0" onAction="#loadPage" prefHeight="25.0" prefWidth="1595.0" />
      <Button fx:id="forwardButton" layoutX="75.0" layoutY="59.0" mnemonicParsing="false" onAction="#nextPage" text="Forward" />
      <Button fx:id="previousButton" layoutX="14.0" layoutY="59.0" mnemonicParsing="false" onAction="#previousPage" text="Previous" />
      <ToolBar layoutX="14.0" layoutY="14.0" prefHeight="40.0" prefWidth="1776.0">
        <items>
          <Button fx:id="historyButton" mnemonicParsing="false" onAction="#displayHistory" prefHeight="25.0" prefWidth="92.0" text="History" />
        </items>
      </ToolBar>
   </children>
</AnchorPane>
